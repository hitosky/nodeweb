{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    ["transform-runtime", { "polyfill": false }],
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-async-to-generator",
    ["import", { "libraryName": "antd" }]
  ],
  "sourceMaps": true,
  "retainLines": true
}
