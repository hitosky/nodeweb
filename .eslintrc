{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "es6": true,
    "node": true
  },
   "rules": {
     "semi":[2,"never"],
     "no-extra-semi":2,
    "no-console":"off",
    "no-extra-bind":"off",
    "react/jsx-no-bind":"off",
    "react/prop-types":"off",
    "no-case-declarations":"off"
  }
}